<%- include('../includes/header') %>
<div class="container" style="min-height: 700px; margin-top: 150px;">
    <div class="row">
        <div class="col-md-12">
            <h4>All Countries Page</h4>
            <table class="table table-dark">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Dial Code</th>
                    <th>Code</th>
                </tr>
                <% result.forEach((value, index)=>{ %>
                    <tr>
                        <td><%- index+1 %></td>
                        <td><%- value.name %></td>
                        <td><%- value.dial_code %></td>
                        <td><%- value.code %></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>
<%- include('../includes/footer') %>