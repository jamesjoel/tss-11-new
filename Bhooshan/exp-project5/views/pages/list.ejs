<%- include("../includes/header") %>

<div class="container my-5" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <h2>List of teacher</h2>
            <table class="table table-dark table-hover">
                <tr>
                    <th>S. nu</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                    <th>Country</th>
                    <th>Delete</th>
                    <th>edit</th>
                </tr>
                <% result.forEach((value, index)=>{ %>
                    <tr>
                    <td><%- index+1 %></td>
                    <td><%- value.name %></td>
                    <td><%- value.email %></td>
                    <td><%- value.contact %></td>
                    <td><%- value.country %></td>
                    <td><a href="/teacher/delete/<%- value._id %>" class="btn btn-danger btn-sm">Delete</a></td>
                    <td><a href="/teacher/edit/<%- value._id %>" class="btn btn-info btn-sm">Edit</a></td>
                    </tr>
                <% }) %>
            </table>

        </div>
    </div>
</div>

<%- include("../includes/footer") %>