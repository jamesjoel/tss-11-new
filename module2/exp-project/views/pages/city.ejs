
<%- include ("../includes/header")%>    

<div class="container" style="min-height : 700px; margin-top: 15px; ">
    <div class="row">
        <div class="col-md-12">
            <h1>Hello citys</h1>
            <table class="table table-dark">
                <tr>
                    <th>id</th>
                    <th>name</th>
                    <th>state</th>
                    
                </tr>
                <% result.forEach((value, index)=>{ %>
                    <tr>
                        <td><%- index+1 %></td>
                        <td><%- value.name %></td>
                        <td><%- value.id %></td>
                        
                    </tr>
                    <% }) %>
            </table>
        </div>
    </div>
</div>

<%- include ("../includes/footer")%>