<%- include("../includes/header") %>
<div class="container my-4" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <h3>List of User</h3>
            <table class="table table-dark table-hover table-bordered">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                    <th>Country</th>
                    <th>Delete</th>
                    <th>Update</th>
                </tr>
                <% result.forEach((value, index)=>{ %>
                    <tr>
                        <td><%- index+1 %></td>
                        <td><%- value.name %></td>
                        <td><%- value.email %></td>
                        <td><%- value.contact %></td>
                        <td><%- value.country %></td>
                        <td><a href="/user/delete/<%- value._id %>" class="btn btn-sm btn-danger">Delete</a></td>
                        <td><a href="/user/update/<%- value._id %>" class="btn btn-sm btn-info">Update</a></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>
<%- include("../includes/footer") %>