<%- include('../includes/header') %>
<div class="container my-5" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <h4 class="text-center">List of All Student</h4>
            <table class="table table-dark table-hover table-striped table-bordered">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Fee</th>
                    <th>Contact</th>
                    <th>Delete</th>
                </tr>
                <% stu.forEach((value, index)=>{ %>
                    <tr>
                        <td><%- index+1 %></td>
                        <td><%- value.fullname %></td>
                        <td><%- value.fee %></td>
                        <td><%- value.contact %></td>
                        <td><a href="/student/delete/<%- value._id %>" class="btn btn-primary btn-sm">Delete</a></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>
<%- include('../includes/footer') %>